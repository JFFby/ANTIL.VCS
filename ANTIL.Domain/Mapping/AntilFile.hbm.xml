<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="ANTIL.Domain"
                   namespace="ANTIL.Domain.Entities">
  <class name="AntilFile" table="[File]">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <property name="Name" not-null="true" />
    <property name="Path" not-null="true" />
    <property name="Updated" not-null="true" />
    <property name="Version" not-null="true" />
  </class>
</hibernate-mapping>
